    <item>
        <title>{% block title %}{% endblock %}</title>
        <description>
            {{ self.content()|escaper }}
        </description>
        <link>{{ ("/notepad.html#%s" % slug)|url(use_base=True) }}</link>
        <guid>{{ slug }}</guid>
        <pubDate>{{ date|datetimeformat }} 00:00:01 PST</pubDate>
    </item>
